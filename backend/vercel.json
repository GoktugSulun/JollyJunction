{
   "version": 2,
   "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "db/**/*",
      "use": "@vercel/node"
    },
    {
      "src": "files/**/*",
      "use": "@vercel/node"
    }
  ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "/"
     }
   ]
 }